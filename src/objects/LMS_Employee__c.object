    <validationRules>
        <fullName>ADHAR_Validation</fullName>
        <active>true</active>
        <description>Please enter Valid Aadhar Card number	Field</description>
        <errorConditionFormula>OR(

AND( LEN( Aadhaar_No__c  ) = 12, NOT(REGEX(  Aadhaar_No__c  ,&quot;^[0-9]+$&quot;)) ),

AND( LEN(  Aadhaar_No__c ) &lt;&gt; 12, REGEX( Aadhaar_No__c  ,&quot;^[0-9]+$&quot;) )

)</errorConditionFormula>
        <errorMessage>Please enter Valid Aadhar Card number	Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PAN_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( REGEX(  PAN_No__c  , &quot;[A-Za-z]{5}[0-9]{4}[A-z]{1}&quot;) )</errorConditionFormula>
        <errorDisplayField>PAN_No__c</errorDisplayField>
        <errorMessage>Please enter Valid PAN Card number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PFValidation</fullName>
        <active>true</active>
        <description>Please enter Valid PF number</description>
        <errorConditionFormula>OR(

AND( LEN(  PF_Account_Number__c ) = 22, NOT(REGEX(  PF_Account_Number__c  ,&quot;^[0-9]+$&quot;)) ),

AND( LEN(  PF_Account_Number__c  ) &lt;&gt; 22, REGEX(  PF_Account_Number__c ,&quot;^[0-9]+$&quot;) )

)</errorConditionFormula>
        <errorMessage>Please enter Valid PF number</errorMessage>
    </validationRules>
